{
  "name": "proavalon_frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "precommit": "lint-staged && npm run build",
    "dev": "next dev",
    "localdev": "env BACKEND_URL=http://localhost:3001 npm run dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint './**/*.{js,jsx,ts,tsx}' --max-warnings 0",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watchAll"
  },
  "lint-staged": {
    "*.json": [
      "prettier --write"
    ],
    "./!(proto)/**/*.{js,jsx,ts,tsx,css,scss}": [
      "prettier --write",
      "npm run lint",
      "npm run test"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.0",
    "@types/jest": "^25.2.1",
    "@types/next-redux-saga": "^3.0.2",
    "@types/react-redux": "^7.1.7",
    "@zeit/next-css": "^1.0.1",
    "axios": "^0.19.2",
    "isomorphic-unfetch": "^3.0.0",
    "jest": "^25.1.0",
    "next": "^9.3.4",
    "next-redux-saga": "^4.1.2",
    "next-redux-wrapper": "^5.0.0",
    "protobufjs": "~6.8.9",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.1.3",
    "semantic-ui-react": "^0.88.2",
    "socket.io-client": "^2.3.0",
    "ts-jest": "^25.2.0"
  },
  "devDependencies": {
    "@types/node": "^13.7.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/socket.io-client": "^1.4.32",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "babel-eslint": "^10.0.3",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^5.0.2",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "typescript": "^3.7.5",
    "url-loader": "^3.0.0"
  }
}
