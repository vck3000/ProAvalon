{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "precommit": "lint-staged && npm run build",
    "prebuild": "rimraf build",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node build/src/main",
    "lint": "eslint \"{src,test}/**/*.ts\" --max-warnings 0",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "dev": "nest start --watch --preserveWatchOutput",
    "test:int:run": "jest --passWithNoTests --forceExit --runInBand --testRegex ispec\\.ts$"
  },
  "lint-staged": {
    "*.json": [
      "prettier --write"
    ],
    "./src/**/*.{js,jsx,ts,tsx,css,scss}": [
      "prettier --write",
      "npm run lint"
    ]
  },
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/platform-socket.io": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@nestjs/websockets": "^7.0.0",
    "@proavalon/proto": "^1.0.0",
    "@typegoose/typegoose": "^6.4.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.6",
    "@types/express-rate-limit": "^5.0.0",
    "@types/helmet": "0.0.45",
    "@types/ioredis": "^4.14.9",
    "@types/jest": "^25.2.1",
    "@types/mongoose": "^5.7.10",
    "@types/node": "^13.11.1",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "@types/socket.io-redis": "^1.0.25",
    "@types/supertest": "^2.0.8",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.2.3",
    "class-transformer-validator": "^0.8.0",
    "class-validator": "^0.12.2",
    "eslint-config-airbnb-base": "^14.1.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.1",
    "helmet": "^3.22.0",
    "ioredis": "^4.16.2",
    "mongoose": "^5.9.7",
    "nestjs-typegoose": "^7.1.14",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "remove": "^0.1.5",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "socket.io-redis": "^5.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^25.3.0",
    "lint-staged": "^10.1.3",
    "mongodb-memory-server": "^6.5.2",
    "node-mocks-http": "^1.8.1",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": "\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFiles": [
      "../.jest/setEnvVars.js"
    ]
  }
}
